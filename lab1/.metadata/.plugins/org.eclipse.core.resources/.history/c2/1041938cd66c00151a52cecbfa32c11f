#include "arbitor.h"

void arbitor::prc_arbi() {
		wait(50);
		selector = false;
		wait(50);
		selector = true;
		wait(50);
		selector = false;
		wait(50);
		selector = true;
		wait(250);
		selector = false;
		wait(300);
		selector = true;
		wait(350);
		selector = false;
		wait(400);
		selector = true;
}
